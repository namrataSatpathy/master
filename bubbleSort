public class Algorithms{
  int arr[] = {5,1,12,-5,16};
  public int[] bubbleSort(int arr[]){
       int length=arr.length();
      for(int i=0; i<length-1; i++)
      {
         for(int j=0;j<length-i-1;j++)
         {
            if(arr[j]>arr[j+1])
            {
              int temp=arr[j];
              arr[j]=arr[j+1];
              arr[j+1]=temp;
             }
          } 
          
        }
        return arr;
     }
}        
            
           
